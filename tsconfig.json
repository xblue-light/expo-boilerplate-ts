{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "./app/*": ["app/*"],
      "./app/components/*": ["app/components/*"],
      "./app/styles/*": ["app/styles/*"],
      "./app/utils/*": ["app/utils/*"]
    },
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "strictNullChecks": false
  }
}
